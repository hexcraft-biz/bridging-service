# Application Setup
APP_HOST=bridging.example.com
APP_PATH=/
APP_PORT=80
GIN_MODE=debug
TIMEZONE=
TRUST_PROXY=localhost

# Database
## Auto create DB schema if not exists: if 'true'. DB_USER has to have the privileges accordingly. (i.e. CREATE database and CREATE table.)
AUTO_CREATE_DB_SCHEMA=false
DB_TYPE=mysql

DB_INIT_USER=user
DB_INIT_PASSWORD=password
DB_INIT_PARAMS="parseTime=true"
DB_USER=user
DB_PASSWORD=password
DB_HOST=localhost
DB_PORT=3306
DB_NAME=bridging_service
DB_PARAMS="parseTime=true"
DB_MAX_OPEN=10
DB_MAX_IDLE=10
DB_LIFE_TIME=120
DB_IDLE_TIME=90

REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_POOL_SIZE=10
REDIS_DIAL_TIMEOUT_SEC=5
REDIS_READ_TIMEOUT_SEC=3
REDIS_WRITE_TIMEOUT_SEC=3
REDIS_POOL_TIMEOUT_SEC=4
REDIS_IDLE_TIMEOUT_SEC=300
